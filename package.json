{
  "name": "vscode-c-esql",
  "displayName": "Embedded SQL in C",
  "description": "Highlight Embedded SQL in C code",
  "version": "0.0.1",
  "publisher": "shanduur",
  "icon": "docs/logo.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#dedede",
    "theme": "light"
  },
  "repository": {
    "url": "https://github.com/Shanduur/c-embedded-sql"
  },
  "bugs": {
    "url": "https://github.com/Shanduur/c-embedded-sql"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "c",
    "sql",
    "embedded-sql",
    "template"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.c",
          "source.sqc",
          "source.ec"
        ],
        "scopeName": "inline.c-sql",
        "path": "./syntaxes/c-embedded-sql.json",
        "embeddedLanguages": {
          "meta.embedded.sql": "sql"
        }
      }
    ]
  }
}